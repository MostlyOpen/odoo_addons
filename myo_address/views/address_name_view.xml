<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        
        <record id="myo_address_view_form_name" model="ir.ui.view">
            <field name="name">myo.address.form</field>
            <field name="model">myo.address</field>
            <field name="inherit_id" ref="myo_address_view_form"/>
            <field name="priority" eval="10" />
            <field name="arch" type="xml">
                <label for="code" position="before">
                    <label for="suggested_name"/>
                    <h3>
                        <field name="suggested_name"/>
                    </h3>
                </label>
                <field name="active" position="after">
                    <field name="automatic_set_name"/>
                </field>
            </field>
        </record>

    </data>
</openerp>
